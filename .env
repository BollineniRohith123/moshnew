# Environment variables for MoshiAI
# Add your Hugging Face token if needed for private models (optional, as Kyutai models are public)
HF_TOKEN=your_huggingface_token_here_if_needed

# GPU settings (set to 0 for single GPU)
CUDA_VISIBLE_DEVICES=0

# WebSocket port (matches app.py)
PORT=8000

# Debug mode
DEBUG=true

# Voice cloning settings
DEFAULT_VOICE=indian_female
SYSTEM_PROMPT=You are a helpful AI assistant with emotional intelligence.
