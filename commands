cd /workspace
git clone https://github.com/devasphn/MoshiAI.git
cd MoshiAI
wget https://huggingface.co/datasets/ai4bharat/indic-tts/resolve/main/samples/indictts_tamil_female.wav -O voices/indian_female.wav
python3 -m venv venv
source venv/bin/activate
apt-get update
apt-get install -y python3-pip python3-dev build-essential libsndfile1-dev ffmpeg portaudio19-dev
pip install --upgrade pip
pip install -r requirements.txt
export CUDA_VISIBLE_DEVICES=0
python app.py
